{
  "openapi": "3.0.2",
  "info": {
    "title": "Cuanmax RESTful API",
    "version": "1.0.0",
    "description": "RESTful API for Cuanmax Application",
    "contact": {
      "name": "Iwan Suryaningrat",
      "email": "iwansuryaningrat@cuanmax.id"
    },
    "license": {
      "name": "APACHE 2.0",
      "url": "https://www.apache.org/licenses/LICENSE-2.0"
    }
  },
  "servers": [
    {
      "description": "Cuanmax RESTful API Server",
      "url": "https://{environment}.cuanmax.id/api/v1",
      "variables": {
        "environment": {
          "description": "Server Environment",
          "default": "dev",
          "enum": ["dev", "stag", "prod"]
        }
      }
    }
  ],
  "paths": {
    "/playlist": {
      "get": {
        "security": [
          {
            "jwt": []
          }
        ],
        "tags": ["palylist"],
        "summary": "Get all playlist",
        "description": "Get all playlist",
        "operationId": "getPlaylist",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array"
                }
              }
            }
          },
          "400": {
            "description": "Invalid ID supplied"
          },
          "404": {
            "description": "Palylist not found"
          }
        }
      }
    }
  }
}
